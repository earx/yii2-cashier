{
    "name": "earx/yii2-cashier",
    "description": "Yii2 Cashier provides an interface to Stripe's, fondy.eu subscription billing services.",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "stripe",
        "billing",
        "fondy"

    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Chepurnoy",
            "email": "igorzfort@gmail.com",
            "role": "creator of original package"
        },
        {
            "name": "Evgeny Bartashevich",
            "email": "ebartashevicvh@gmail.com",
            "role": "creator of fondy package"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "cloudipsp/ipsp-php",
                "version": "0.0.1",
                "source": {
                    "url": "https://github.com/cloudipsp/ipsp-php",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "yiisoft/yii2": ">=2.0.8",
        "yii2mod/yii2-behaviors": "*",
        "stripe/stripe-php": "~4.0",
        "nesbot/carbon": "~1.0",
        "dompdf/dompdf": "^0.8.0",
        "cloudipsp/ipsp-php": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "earx\\cashier\\": ""
        }
    }
}
